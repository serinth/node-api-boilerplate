{
  "name": "node-api-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src/app.js\"",
    "build": "rm -rf ./dist/* && babel src --out-dir dist",
    "lint": "./node_modules/.bin/eslint src",
    "start": "node -r esm dist/app.js",
    "test": "./node_modules/.bin/_mocha -r esm src/**/*.spec.js",
    "ci-test-cover": "node_modules/.bin/babel-node node_modules/.bin/isparta cover --include 'src/**/*.js' --excludes 'src/**/*.spec.js' --report html --report text node_modules/mocha/bin/_mocha -- 'src/**/*.spec.js'"
  },
  "pre-commit": [
    "lint",
    "ci-test-cover"
  ],
  "author": "Tony Truong",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "chai": "^3.5.0",
    "eslint": ">=4.18.2",
    "eslint-config-google": "^0.9.1",
    "isparta": "^4.1.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^1.8.0",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-winston": "^4.0.1",
    "lodash": "^4.17.15",
    "winston": "^3.2.1"
  }
}
